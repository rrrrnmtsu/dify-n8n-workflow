# ======================
# Database Configuration
# ======================
POSTGRES_USER=dify
POSTGRES_PASSWORD=dify_password_changeme
POSTGRES_DB=dify

# n8n Database
N8N_DB=n8n

# ======================
# Redis Configuration
# ======================
REDIS_PASSWORD=redis_password_changeme

# ======================
# Dify Configuration
# ======================
DIFY_SECRET_KEY=your-secret-key-changeme-at-least-32-chars
DIFY_MODE=production
LOG_LEVEL=INFO

# Dify API URL (外部からアクセスする場合は適切なURLに変更)
DIFY_API_URL=http://localhost:5001
DIFY_CONSOLE_URL=http://localhost:3000

# Dify Ports
DIFY_API_PORT=5001
DIFY_WEB_PORT=3000

# ======================
# n8n Configuration
# ======================
N8N_BASIC_AUTH_ACTIVE=true
N8N_USER=admin
N8N_PASSWORD=admin_password_changeme

# n8n Webhook Configuration
N8N_WEBHOOK_URL=http://localhost:5678
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http

# n8n Execution Configuration
N8N_EXECUTIONS_MODE=regular
N8N_EXECUTIONS_TIMEOUT=300
N8N_EXECUTIONS_TIMEOUT_MAX=3600

# ======================
# General Configuration
# ======================
TIMEZONE=Asia/Tokyo

# ======================
# API Keys (必要に応じて設定)
# ======================
# OpenAI API Key
# OPENAI_API_KEY=sk-...

# Anthropic API Key
# ANTHROPIC_API_KEY=sk-ant-...

# その他のAPI Keys
# GOOGLE_API_KEY=
# AZURE_OPENAI_API_KEY=
